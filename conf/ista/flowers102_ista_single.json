{   
    "seed": 5,
    "gpu": "0",
    "cudnn_enabled": true,
    "cudnn_benchmark": true,
    "save_root": "train_model",
    "save_name": "",
    "load_name": "",
    "model_path": "",
    "save_freq": 50,
    "all_freeze": false,
    "dataset": {
        "name": "flowers102",
        "root": "/data/flowers102/",
        "root_cloud": "/cache/dataset/flowers102/",
        "num_class": 102,
        "mean": [0.485, 0.456, 0.406],
        "std": [0.229, 0.224, 0.225],
        "img_min_size": 224,
        "val_ratio": 0.02,
        "cutout_size": 112,
        "batch_size": 96,
        "workers": 16, 
        "pin": false,
        "random_sample": true,
        "real_val": false
    },
    "train": {
        "help": "Training original darts.",
        "start_epoch": 0,
        "epochs": 600,
        "retrain_epochs": 600,
        "w_lr": 0.025,
        "w_lr_min": 0.001,
        "w_momentum": 0.9,
        "w_nesterov": true,
        "w_weight_decay": 3e-4,
        "w_grad_clip": 5,
        "print_freq": 25
    },
    "train_architecture":{
        "help": "Training original dart's architecture.",
        "input_size": 224,
        "input_channels": 3,
        "init_channels": 36,
        "n_classes": 102,
        "layers": 20,
        "alpha_lr": 3e-4,
        "alpha_weight_decay": 1e-3,
        "drop_path_prob": 0.2,
        "steps": 4, 
        "-help-": "Steps mean the number of nodes in a cell.",
        "proj_dims": 7,
        "sparseness": 2,
        "param_limit": 6.0,
        "auxiliary": true,
        "aux_weight": 0.4,
        "dist_limit": 1e-4
    },
    "hpo":{
        "help": "Method for train_p, val_h",
        "h_lr": 5e-8,
        "h_lr_tune": 5e-7,
        "h_momentum": 0.9,
        "h_nesterov": true,
        "h_weight_decay": 0.0005,
        "weight_decay_low": 1e-4, 
        "weight_decay_up": 5e-4,
        "learning_low": 0.00001,
        "learning_up": 0.05
    },
    "da":{
        "help": "Method for train_p, val_h",
        "policy": 14,
        "d_lr": 0.005,
        "d_lr_tune": 0.05,
        "d_momentum": 0.9,
        "d_nesterov": true,
        "d_weight_decay": 0.0005,
        "temperature": 2
    }
}
